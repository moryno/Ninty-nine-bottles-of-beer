 

function beer(){
   var output = [];
   var count = 99;
    while(count >= 0){
        if(count !== 0){
            output.push(count + " bottles of beer on the wall, " + count + "bottles of beer. Take one down and pass it around, " + count-- + " bottles of beer on the wall.");
            
        }else{
            output.push(count);
        }
        count--;
    }
   console.log(output);
}




